<p id="notice"><%= notice %></p>

<h1>Game Session Players</h1>

<table>
  <thead>
    <tr>
      <th>Game session</th>
      <th>Player</th>
      <th>Won</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @game_session_players.each do |game_session_player| %>
      <tr>
        <td><%= game_session_player.game_session_id %></td>
        <td><%= game_session_player.player_id %></td>
        <td><%= game_session_player.won %></td>
        <td><%= link_to 'Show', game_session_player %></td>
        <td><%= link_to 'Edit', edit_game_session_player_path(game_session_player) %></td>
        <td><%= link_to 'Destroy', game_session_player, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Game Session Player', new_game_session_player_path %>
